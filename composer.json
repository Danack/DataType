{

  "name": "danack/typespec",
  "description": "A library that makes programatic types easier",
  "license": "MIT",
  "require": {
    "php" : "^8.0",
    "ext-mbstring": "*",
    "danack/var-map": "^0.6.1",
    "danack/json-safe":  "^0.1.0",
    "laminas/laminas-diactoros": "^2.6"
  },
  "require-dev": {
    "danack/coding-standard": "0.3.0",
    "danack/php-unit-helper": "^0.3.1",
    "infection/infection": "^0.23.0",
    "phpunit/phpunit": "9.5.6",
    "phpstan/phpstan-strict-rules": "^1.1.0",
    "psr/http-message": "^1.0",
    "respect/validation":  "^1.1.31",
    "slevomat/coding-standard": "^7.0.9",
    "squizlabs/php_codesniffer": "^3.6.0",
    "yoast/yoastcs": "1.0"
  },
  "autoload": {
    "files": [
      "src/TypeSpec/functions.php"
    ],
    "psr-0": {
      "TypeSpec\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-0": {
      "TypeSpecTest\\": "test/",
      "TypeSpecExample\\": "example/"
    }
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    }
  },
  "prefer-stable": true
}
