{

  "name": "danack/params",
  "description": "Converts user input into correctly typed parameters",
  "license": "MIT",
  "require": {
    "php" : "^7.4",
    "ext-mbstring": "*",
    "danack/var-map": "^0.6.0"
  },
  "require-dev": {
    "danack/coding-standard": "0.2.0",
    "infection/infection": "^0.15.3",
    "phpunit/phpunit": "9.0.1",
    "phpstan/phpstan-strict-rules": "^0.11",
    "psr/http-message": "^1.0",
    "respect/validation":  "^1.1.31",
    "slevomat/coding-standard": "^4.8.4",
    "squizlabs/php_codesniffer": "^3.3.1",
    "yoast/yoastcs": "1.0"
  },
  "autoload": {
    "files": [
      "lib/Params/functions.php"
    ],
    "psr-0": {
      "Params\\": "lib/"
    }
  },
  "autoload-dev": {
    "psr-0": {
      "ParamsTest\\": "test/",
      "ParamsExample\\": "example/"
    }
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    }
  },
  "prefer-stable": true
}
